{
  "name": "fit-viewer",
  "displayName": "FIT File Viewer",
  "description": "A VSCode extension to preview .fit files",
  "version": "0.6.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/thomascamminady/fit-viewer"
  },
  "icon": "icon.png",
  "publisher": "thomascamminady",
  "author": "Thomas Camminady",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "customEditors": [
      {
        "viewType": "fitViewer.fitEditor",
        "displayName": "FIT File Viewer",
        "selector": [
          {
            "filenamePattern": "*.fit"
          }
        ]
      }
    ]
  },
  "main": "./out/extension.js",
  "type": "module",
  "scripts": {
    "compile": "tsc -p ./ && npm run copy-assets",
    "copy-assets": "copyfiles -u 1 media/* out/media/",
    "package": "vsce package",
    "build": "electron-builder --projectDir=electron-app"
  },
  "build": {
    "directories": {
      "app": "electron-app",
      "output": "dist"
    },
    "files": [
      "**/*"
    ],
    "win": {
      "target": "nsis"
    }
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@eslint/css": "^0.7.0",
    "@eslint/js": "^9.25.1",
    "@eslint/json": "^0.12.0",
    "@eslint/markdown": "^6.4.0",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.3",
    "@types/vscode": "^1.99.1",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@vscode/vsce": "^3.3.2",
    "clang-format": "^1.8.0",
    "copyfiles": "^2.4.1",
    "electron-builder": "^26.0.12",
    "eslint": "^9.25.1",
    "eslint-define-config": "^2.1.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jsdom": "^26.1.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  },
  "dependencies": {
    "@garmin/fitsdk": "^21.170.0",
    "arquero": "^8.0.1"
  }
}
