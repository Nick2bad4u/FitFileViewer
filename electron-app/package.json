{
  "name": "fitfileviewer",
  "version": "1.5.0",
  "main": "main.js",
  "displayName": "Fit File Viewer",
  "productName": "Fit File Viewer",
  "build": {
    "icon": "../icon.png",
    "appId": "com.example.fitfileviewer",
    "publish": [
      {
        "provider": "github",
        "owner": "Nick2bad4u",
        "repo": "FitFileViewer"
      }
    ]
  },
  "scripts": {
    "test": "jest",
    "start": "electron . --inspect=9229",
    "build": "electron-builder",
    "package": "electron-builder --dir",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e"
  },
  "keywords": [
    "fit-file",
    "viewer",
    "fit",
    "data-visualization",
    "electron",
    "garmin",
    "charts",
    "maps",
    "tables"
  ],
  "author": "Nick2bad4u",
  "homepage": "https://github.com/Nick2bad4u/FitFileViewer",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nick2bad4u/FitFileViewer.git"
  },
  "bugs": {
    "url": "https://github.com/Nick2bad4u/FitFileViewer/issues"
  },
  "license": "ISC",
  "description": "Fit File Viewer is a cross-platform application for parsing, visualizing, and analyzing .fit files using charts, maps, and tables.",
  "devDependencies": {
    "electron": "^36.0.1",
    "electron-builder": "^26.0.12",
    "eslint": "^9.16.0",
    "jest": "^29.7.0"
  },
  "dependencies": {
    "@garmin/fitsdk": "^21.170.0"
  }
}
