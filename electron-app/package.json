    "test:watch": "vitest --watch",
    "test:watch-changed": "vitest --watch --changed",
    "test:coverage": "cross-env NODE_OPTIONS=--max-old-space-size=8192 vitest --coverage --run --reporter=dot --silent",
    "test:coverage:enhanced": "cross-env NODE_OPTIONS=--max-old-space-size=8192 vitest --config vitest.config.enhanced.js --coverage --run --reporter=dot --silent",
    "test:enhanced": "cross-env NODE_OPTIONS=--max-old-space-size=8192 vitest --config vitest.config.enhanced.js --run --reporter=dot --silent",
