{
    "$schema": "https://unpkg.com/knip@5/schema.json",
    "entry": [
        "index.html!",
        "main.js!",
        "renderer.js!",
        "preload.js!",
        "utils.js!",
        "fitParser.js!"
    ],
    "project": [
        "**/*.{js,ts,jsx,tsx}",
        "!**/*.d.ts"
    ],
    "ignore": [
        "**/__mocks__/**",
        "**/coverage/**",
        "**/dist/**",
        "**/node_modules/**",
        "**/*.test.{js,ts}",
        "**/*.spec.{js,ts}",
        "**/tests/**",
        "**/*.d.ts",
        "**/ffv/**",
        "**/knip.json"
    ],
    "ignoreDependencies": [
        "@maplibre/maplibre-gl-leaflet",
        "arquero",
        "chart.js",
        "chartjs-adapter-date-fns",
        "chartjs-plugin-zoom",
        "datatables.net",
        "datatables.net-dt",
        "date-fns",
        "hammerjs",
        "jquery",
        "jszip",
        "leaflet",
        "leaflet-draw",
        "leaflet-measure",
        "leaflet-minimap",
        "leaflet.fullscreen",
        "leaflet.locatecontrol",
        "leaflet.markercluster",
        "maplibre-gl",
        "@actions/artifact",
        "@actions/attest",
        "@actions/cache",
        "@actions/core",
        "@actions/exec",
        "@actions/github",
        "@actions/glob",
        "@actions/http-client",
        "@actions/io",
        "@actions/tool-cache",
        "@kurkle/color",
        "@microsoft/eslint-formatter-sarif",
        "@types/mocha",
        "@typescript-eslint/eslint-plugin",
        "color",
        "electron-builder-squirrel-windows",
        "git-cliff",
        "makensis",
        "postcss",
        "postcss-value-parser",
        "prettier-eslint",
        "prettier-plugin-ini",
        "prettier-plugin-interpolated-html-tags",
        "prettier-plugin-jsdoc",
        "prettier-plugin-merge",
        "prettier-plugin-multiline-arrays",
        "prettier-plugin-packagejson",
        "prettier-plugin-properties",
        "prettier-plugin-sort-json",
        "prettier-plugin-sql",
        "prettier-plugin-tailwindcss",
        "stylelint-config-standard",
        "ts-migrate",
        "ts-migrate-plugins"
    ],
    "ignoreExportsUsedInFile": true,
    "ignoreMembers": [
        "__clearAllListenersForTests",
        "__debugState",
        "__TEST_ONLY_.*"
    ],
    "ignoreBinaries": [],
    "includeEntryExports": true,
    "rules": {
        "files": "warn",
        "dependencies": "warn",
        "devDependencies": "warn",
        "unlisted": "error",
        "exports": "warn",
        "types": "warn",
        "nsExports": "warn",
        "nsTypes": "warn",
        "enumMembers": "warn",
        "classMembers": "warn",
        "duplicates": "error"
    }
}
